ASSEMBLER INVOKED BY: C:\Keil_v5\C51\BIN\A51.EXE u_char.asm SET(SMALL) DEBUG PRINT(.\Listings\u_char.lst) OBJECT(.\Objec
                      ts\u_char.obj) EP

LOC  OBJ            LINE     SOURCE

0000                   1     ORG 0x0000 ; Start the program at address 0
                       2     
                       3     ; Define constants
  0099                 4     UART_DATA equ 0x99   ; UART Data Register
  0098                 5     UART_CTRL equ 0x98   ; UART Control Register
                       6     
                       7     ; Initialize the serial port for 9600 baud
0000 758920            8     MOV TMOD, #0x20      ; Timer 1, Mode 2 (8-bit auto-reload)
0003 758DFD            9     MOV TH1, #0xFD       ; Set the baud rate for 9600 baud (for 11.0592 MHz crystal)
0006 D28E             10     SETB TR1             ; Start Timer 1
0008 D299             11     SETB TI              ; Set TI (Transmit Interrupt) to enable transmission
                      12     
000A                  13     MAIN:
                      14       ; Continuously transmit 'U'
000A 7455             15       MOV A, #'U'        ; Load 'U' into the accumulator
000C F599             16       MOV SBUF, A        ; Send 'U' to UART Data Register
000E                  17       HERE:
000E 3099FD           18         JNB TI, HERE     ; Wait for TI to be set (transmission complete)
0011 C299             19         CLR TI            ; Clear TI for the next transmission
0013 80F5             20         SJMP MAIN        ; Repeat the loop indefinitely
                      21     end
